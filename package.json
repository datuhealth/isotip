    }
  ],
  "license": "Apache-2.0",
  "devDependencies": {
    "browserify": "^13.0.0",
    "chai": "^3.3.0",
    "derequire": "^2.0.2",
    "es5-shim": "^4.0.6",
    "gulp": "^3.8.10",
    "gulp-logwarn": "0.0.8",
    "gulp-mocha-phantomjs": "^0.10.1",
    "mocha": "^2.1.0",
